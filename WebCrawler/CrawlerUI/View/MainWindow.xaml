<Window x:Class="CrawlerUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CrawlerUI"
        xmlns:ViewModelNamespace="clr-namespace:CrawlerUI.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <ViewModelNamespace:CrawlerUnit x:Key="CrawlerUnit"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource CrawlerUnit}">
        <TextBlock Text="{Binding CrawlerUnit.CrawlResult, UpdateSourceTrigger=PropertyChanged}" FontWeight="Bold" Margin="131,73,119,81"/>
    </Grid>
</Window>
